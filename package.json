{
  "name": "case-anything",
  "version": "1.1.2",
  "sideEffects": false,
  "description": "camelCase, kebab-case, PascalCase... a simple integration with nano package size. (SMALL footprint!)",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "types/index.d.ts",
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "ava",
    "rollup": "rollup -c build.js",
    "build": "rm -rf ./dist && npm run lint && npm run rollup && npm run test"
  },
  "dependencies": {},
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "ava": "^3.12.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-tree-shaking": "^1.8.0",
    "rollup": "^2.26.8",
    "rollup-plugin-typescript2": "^0.27.2",
    "tsconfig-paths": "^3.9.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mesqueeb/case-anything.git"
  },
  "keywords": [
    "change-case",
    "change-casing",
    "case-change",
    "casing-change",
    "camel-case",
    "pascal-case",
    "kebab-case",
    "snake-case",
    "constant-case",
    "path-case",
    "camel",
    "pascal",
    "kebab",
    "snake",
    "constant",
    "path",
    "format-string"
  ],
  "author": "Luca Ban - Mesqueeb",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mesqueeb/case-anything/issues"
  },
  "homepage": "https://github.com/mesqueeb/case-anything#readme",
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "tsconfig-paths/register",
      "ts-node/register"
    ]
  }
}
