{
  "name": "case-anything",
  "version": "1.1.0",
  "sideEffects": false,
  "description": "camelCase, kebab-case, PascalCase... a simple integration with nano package size. (SMALL footprint!)",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "types/index.d.ts",
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "ava",
    "rollup": "rollup -c build/rollup.js",
    "build": "npm run lint && npm run rollup && npm run test"
  },
  "dependencies": {},
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "ava": "^3.5.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-tree-shaking": "^1.8.0",
    "rollup": "^1.32.1",
    "rollup-plugin-typescript2": "^0.25.3",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mesqueeb/case-anything.git"
  },
  "keywords": [
    "change-case",
    "change-casing",
    "case-change",
    "casing-change",
    "camel-case",
    "pascal-case",
    "kebab-case",
    "snake-case",
    "constant-case",
    "path-case",
    "camel",
    "pascal",
    "kebab",
    "snake",
    "constant",
    "path",
    "format-string"
  ],
  "author": "Luca Ban - Mesqueeb",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mesqueeb/case-anything/issues"
  },
  "homepage": "https://github.com/mesqueeb/case-anything#readme",
  "ava": {
    "extensions": ["ts"],
    "require": ["ts-node/register"]
  }
}
